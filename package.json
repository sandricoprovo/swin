{
    "name": "swin",
    "version": "1.0.0",
    "description": "Swin is an app where you can find encouragement to complete your goals from start to finish.",
    "scripts": {
        "install:all": "pnpm recursive install",
        "build": "pnpm --filter @swin/server --filter @swin/client recursive run build",
        "dev": "pnpm --filter @swin/server --filter @swin/client recursive run dev",
        "client:start": "pnpm --filter @swin/client recursive run start",
        "client:dev": "pnpm --filter @swin/client recursive run dev",
        "client:build": "pnpm --filter @swin/client recursive run build",
        "server:start": "pnpm --filter @swin/server recursive run build",
        "server:dev": "pnpm --filter @swin/server recursive run dev",
        "server:build": "pnpm --filter @swin/server recursive run build",
        "infra:start": "pnpm --filter @swin/server recursive run infra:start",
        "infra:stop": "pnpm --filter @swin/server recursive run infra:stop"
    },
    "keywords": [
        "react",
        "typescript",
        "node",
        "express"
    ],
    "author": "Sandrico Provo",
    "license": "ISC",
    "devDependencies": {
        "@babel/core": "^7.21.0",
        "@babel/eslint-parser": "^7.19.1",
        "@babel/preset-react": "^7.18.6",
        "@types/node": "^18.14.2",
        "@typescript-eslint/eslint-plugin": "^5.54.0",
        "@typescript-eslint/parser": "^5.54.0",
        "eslint": "^8.35.0",
        "eslint-config-airbnb": "^19.0.4",
        "eslint-config-airbnb-typescript": "^17.0.0",
        "eslint-config-sandricoprovo": "^4.0.0",
        "eslint-import-resolver-typescript": "^3.5.3",
        "eslint-plugin-html": "^7.1.0",
        "eslint-plugin-import": "^2.27.5",
        "eslint-plugin-jsx-a11y": "^6.7.1",
        "eslint-plugin-react": "^7.32.2",
        "eslint-plugin-react-hooks": "^4.6.0",
        "typescript": "^4.9.5"
    }
}
